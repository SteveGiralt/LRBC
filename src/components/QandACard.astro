---
const { q } = Astro.props;
---

<div class="w-full max-w-5xl rounded-lg px-1 pt-1 mx-auto my-1">
  <div class="items-center pb-10">
    <h5 class="mb-1 text-2xl font-medium text-gray-900 font-oswald text-center">
      {q}
    </h5>
    <div class="mt-4 md:mt-6 px-2">
      <slot />
    </div>
  </div>
</div>
